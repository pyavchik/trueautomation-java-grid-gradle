    <test name="Edge">
        <parameter name="browser" value="MicrosoftEdge"/>
        <classes>
        <class name="exampleTest"/>
        </classes>
    </test>
    <test name="IE">
        <parameter name="browser" value="internet explorer"/>
        <classes>
        <class name="exampleTest"/>
        </classes>
    </test>
</suite>